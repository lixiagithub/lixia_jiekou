from time import sleep, ctime


def loop0():
    print('start loop 0 at:%r' % ctime())
    sleep(4)
    print('loop 0 done at:%r' % ctime())


def loop1():
    print('start loop 1 at:%r' % ctime())
    sleep(2)
    print('loop 1 done at:%r' % ctime())


def main():
    print('start:%r' % ctime())
    loop0()
    loop1()
    print('all end:%r' % ctime())

if __name__ == '__main__':
    main()
